<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_land_graph"
    app:startDestination="@id/LoginFragment">

    <fragment android:id="@+id/LoginFragment"
        android:name="com.mosc.simo.ptuxiaki3741.fragments.LoginRegisterFragment"
        android:label="fragment_login_register"
        tools:layout="@layout/fragment_login_register" >
        <action
            android:id="@+id/toMenu"
            app:destination="@id/MainMenuFragment"
            app:popUpTo="@id/nav_land_graph"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment android:id="@+id/MainMenuFragment"
        android:name="com.mosc.simo.ptuxiaki3741.fragments.MainMenuFragment"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu_main" >
        <action
            android:id="@+id/toLogin"
            app:destination="@id/LoginFragment"
            app:popUpTo="@id/nav_land_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/toUserProfile"
            app:destination="@id/UserProfileFragment" />
        <action
            android:id="@+id/toLandHistory"
            app:destination="@id/LandHistoryFragment" />
        <action
            android:id="@+id/toListMenu"
            app:destination="@id/LandCollectionFragment" />
        <action
            android:id="@+id/toUserContacts"
            app:destination="@id/UserContactsFragment" />
    </fragment>
    <fragment android:id="@+id/LandInfoFragment"
        android:name="com.mosc.simo.ptuxiaki3741.fragments.LandInfoFragment"
        android:label="fragment_land_info"
        tools:layout="@layout/fragment_land_info" >
        <action
            android:id="@+id/toLandMap"
            app:destination="@id/LandMapFragment"
            app:popUpTo="@id/LandCollectionFragment"
            app:popUpToInclusive="false" />
        <argument
            android:name="land"
            app:argType="com.mosc.simo.ptuxiaki3741.models.Land" />
    </fragment>
    <fragment android:id="@+id/LandMapFragment"
        android:name="com.mosc.simo.ptuxiaki3741.fragments.LandMapFragment"
        android:label="fragment_land_map"
        tools:layout="@layout/fragment_land_map" >
        <action
            android:id="@+id/toListMenu"
            app:destination="@id/LandCollectionFragment"
            app:popUpTo="@id/LandCollectionFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/toLandInfo"
            app:destination="@id/LandInfoFragment" />
        <argument
            android:name="land"
            app:argType="com.mosc.simo.ptuxiaki3741.models.Land" />
        <argument
            android:name="address"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />
        <argument
            android:name="displayMode"
            app:argType="boolean"
            android:defaultValue="false" />
    </fragment>
    <fragment android:id="@+id/UserProfileFragment"
        android:name="com.mosc.simo.ptuxiaki3741.fragments.UserProfileFragment"
        android:label="fragment_user_profile"
        tools:layout="@layout/fragment_user_profile" >
        <action
            android:id="@+id/toLogin"
            app:destination="@id/LoginFragment"
            app:popUpTo="@id/nav_land_graph"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment android:id="@+id/LandHistoryFragment"
        android:name="com.mosc.simo.ptuxiaki3741.fragments.HistoryMenuFragment"
        android:label="fragment_land_history_menu"
        tools:layout="@layout/fragment_menu_history" >
        <action
            android:id="@+id/toLandMap"
            app:destination="@id/LandMapFragment" />
    </fragment>
    <fragment android:id="@+id/LandCollectionFragment"
        android:name="com.mosc.simo.ptuxiaki3741.fragments.LandMenuFragment"
        android:label="fragment_land_collection"
        tools:layout="@layout/fragment_menu_land" >
        <action
            android:id="@+id/toLandInfo"
            app:destination="@id/LandInfoFragment" />
        <action
            android:id="@+id/toLandMap"
            app:destination="@id/LandMapFragment" />
    </fragment>
    <fragment
        android:id="@+id/UserContactsFragment"
        android:name="com.mosc.simo.ptuxiaki3741.fragments.UserContactsFragment"
        android:label="fragment_user_contacts"
        tools:layout="@layout/fragment_user_contacts" />
</navigation>